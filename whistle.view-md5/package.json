{
  "name": "whistle.view-md5",
  "version": "1.0.0",
  "description": "该插件是在 Network / Inspectors 自定义 Tab 的示例（whistle >= `v2.8.9`），通过在该自定义的 Tab 可以查看请求 URL 和请求内容，响应内容的 MD5",
  "whistleConfig": {
    "inspectorTabs": {
      "page": "req-tab.html",
      "name": "测试自定义 Inspector Tab",
      "req": {
        "page": "req-tab.html",
        "name": "请求数据 MD5"
      },
      "res": {
        "action": "res-tab.html",
        "name": "响应数据 MD5"
      }
    },
    "composerTab": {
      "action": "res-tab.html",
      "name": "测试自定义 Composer Tab"
    }
  },
  "dependencies": {
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0"
  }
}